       "\t\t\t", 
       RowBox[{"If", "[", 
        RowBox[{"async", ",", "\n", "\t\t\t\t", 
         RowBox[{
          RowBox[{"PreemptProtect", "[", 
           RowBox[{"(*", 
            RowBox[{
            "Use", " ", "PreemptProtect", " ", "to", " ", "ensure", " ", 
             "that", " ", 
             RowBox[{"setTaskData", "[", "]"}], " ", "\n", "\t\t\t\t\t", 
             "gets", " ", "called", " ", "before", " ", 
             "pacletDownloadCallback", " ", "can", " ", 
             RowBox[{"fire", "."}]}], "*)"}], "\n", "\t\t\t\t\t", 
           RowBox[{
            RowBox[{"downloadTask", "=", "\n", "\t\t\t\t", 
             RowBox[{"URLSaveAsynchronous", "[", "\n", "\t\t\t\t\t\t", 
              RowBox[{
              "truePaclet", ",", "\n", "\t\t\t\t\t\t", "downloadedFileName", 
               ",", "\n", "\t\t\t\t\t\t", 
               "PacletManager`Manager`Private`pacletDownloadCallback", ",", 
               "\n", "\t\t\t\t\t\t", 
               RowBox[{"\"\<Headers\>\"", "\[Rule]", "\n", "\t\t\t\t\t\t\t", 
                RowBox[{"{", "\n", "\t\t\t\t\t\t\t\t", 
                 RowBox[{
                  RowBox[{
                  "\"\<Mathematica-systemID\>\"", "\[Rule]", "$SystemID"}], 
                  ",", "\n", "\t\t\t\t\t\t\t\t", 
                  RowBox[{"\"\<Mathematica-license\>\"", "\[Rule]", 
                   RowBox[{"ToString", "[", "$LicenseID", "]"}]}], ",", "\n", 
                  "\t\t\t\t\t\t\t\t", 
                  RowBox[{"\"\<Mathematica-mathID\>\"", "\[Rule]", 
                   RowBox[{"ToString", "[", "$MachineID", "]"}]}], ",", "\n", 
                  "\t\t\t\t\t\t\t\t", 
                  RowBox[{"\"\<Mathematica-language\>\"", "\[Rule]", 
                   RowBox[{"ToString", "[", "$Language", "]"}]}], ",", "\n", 
                  "\t\t\t\t\t\t\t\t", 
                  RowBox[{"\"\<Mathematica-activationKey\>\"", "\[Rule]", 
                   RowBox[{"ToString", "[", "$ActivationKey", "]"}]}]}], "\n",
                  "\t\t\t\t\t\t\t\t", "}"}]}], ",", "\n", "\t\t\t\t\t\t", 
               RowBox[{
               "\"\<UserAgent\>\"", "\[Rule]", 
                "PacletManager`Package`$userAgent"}], ",", "\n", 
               "\t\t\t\t\t\t", 
               RowBox[{"BinaryFormat", "\[Rule]", "True"}], ",", "\n", 
               "\t\t\t\t\t\t", 
               RowBox[{"\"\<Progress\>\"", "\[Rule]", "True"}]}], "\n", 
              "\t\t\t\t\t\t", "]"}]}], ";", "\n", "\t\t\t\t\t", 
            RowBox[{"PacletManager`Package`setTaskData", "[", 
             RowBox[{"downloadTask", ",", "\n", "\t\t\t\t\t\t", 
              RowBox[{"{", "\n", "\t\t\t\t\t\t\t", 
               RowBox[{
               "pName", ",", "\n", "\t\t\t\t\t\t\t", "downloadedFileName", 
                ",", "\n", "\t\t\t\t\t\t\t", "loc", ",", "\n", 
                "\t\t\t\t\t\t\t", "\"\<Running\>\"", ",", "\n", 
                "\t\t\t\t\t\t\t", 
