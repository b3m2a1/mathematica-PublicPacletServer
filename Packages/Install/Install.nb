      RowBox[{"pName", "<>", "\"\<.paclet\>\""}]}], ";", "\n", "\t\t", 
     RowBox[{"truePaclet", "=", "\n", "\t\t  ", 
      RowBox[{"Which", "[", "\n", "\t\t    ", 
       RowBox[{
        RowBox[{"StringEndsQ", "[", 
         RowBox[{"loc", ",", " ", "pacletFileName"}], "]"}], ",", "\n", 
