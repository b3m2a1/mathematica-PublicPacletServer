Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"Options", "[", "downloadRawPacletsToo", "]"}], " ", "=", " ", 
   "\n", "\t", 
   RowBox[{"{", 
    RowBox[{"\"\<CompletionFunction\>\"", " ", "->", " ", "None"}], "}"}]}], 
  ";"}], "\n", 
 RowBox[{
  RowBox[{"downloadRawPacletsToo", "[", "\n", "\t", 
   RowBox[{"remotePaclet_PacletManager`Paclet", ",", "\n", "\t", 
    RowBox[{"async", ":", 
     RowBox[{"(", 
      RowBox[{"True", "|", "False"}], ")"}], ":", "True"}], ",", "\n", "\t", 
    RowBox[{"OptionsPattern", "[", "]"}]}], "\n", "\t", "]"}], ":=", "\n", 
  "\t", 
  RowBox[{"Module", "[", "\n", "\t\t", 
   RowBox[{
    RowBox[{"{", "\n", "\t\t\t", 
     RowBox[{
     "loc", ",", "downloadTask", ",", "pacletFileName", ",", 
      "downloadedFileName", ",", "\n", "\t\t\t", "truePaclet", ",", " ", 
      "pName"}], "\n", "\t\t\t", "}"}], ",", "\n", "\t\t", 
    RowBox[{
     RowBox[{"loc", "=", 
      RowBox[{
      "PacletManager`Package`PgetLocation", "[", "remotePaclet", "]"}]}], ";",
      "\n", "\t\t", 
